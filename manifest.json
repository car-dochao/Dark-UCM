{
    "manifest_version": 3,
    "name": "Dark-UCM (Legacy)",
    "description": "Dark theme for the (old) UCM Virtual Campus",
    "icons": { "96" : "icon.png" },
    "version": "0.6.1",
    "author": "Carlos Dochao",
    "homepage_url": "https://github.com/car-dochao/Dark-UCM/tree/legacy",
    "host_permissions": ["https://cvant.ucm.es/*"],
    
    "content_scripts": [{
        "matches": ["https://cvant.ucm.es/*"],
        "exclude_globs": ["*login/index.php*"],
        "css": ["main.css"]
    }]
}